    {
        "id": "b1401ad5ac2ac853",
        "type": "function",
        "z": "124a124ed5371393",
        "name": "indicação com incerteza",
        "func": "// Valor da incerteza (pode ser ajustado)\nconst inc = 0.5;\n\n// Converte o valor atual recebido\nconst valor = Number(msg.payload);\nif (isNaN(valor)) return null;\n\n// Timestamp atual em formato ISO\nconst ts = new Date().toISOString();\n\n// Cria 3 mensagens com tópicos distintos\nlet medida = {\n    topic: \"Valor Medido\",\n    payload: valor,\n    timestamp: ts\n};\n\nlet medidaMais = {\n    topic: \"Limite Superior (±0.5 cm)\",\n    payload: valor + inc,\n    timestamp: ts\n};\n\nlet medidaMenos = {\n    topic: \"Limite Inferior (±0.5 cm)\",\n    payload: valor - inc,\n    timestamp: ts\n};\n\n// Envia as três curvas para o gráfico\nreturn [medida, medidaMais, medidaMenos];",
